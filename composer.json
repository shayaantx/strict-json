{
  "name": "sburba/strict-json",
  "description": "A strict and basic json parser",
  "type": "library",
  "license": "MIT",
  "config": {
    "platform": {
      "php": "8.1.0"
    },
    "allow-plugins": {
      "infection/extension-installer": false
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "infection/infection": "^0.27.6",
    "friendsofphp/php-cs-fixer": "^3.36",
    "couscous/couscous": "^1.7",
    "phpbench/phpbench": "^1.2.14",
    "phpstan/phpstan": "^1.10.39",
    "phpstan/phpstan-phpunit": "^1.3.15",
    "phpstan/phpstan-strict-rules": "^1.5.1"
  },
  "autoload": {
    "psr-4": {
      "Burba\\StrictJson\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Burba\\StrictJson\\": "test/"
    }
  },
  "require": {
    "ext-json": "*"
  }
}
